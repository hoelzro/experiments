%! draw some rectangles of different colors

0 1 4
{
  newpath
  dup
  150 mul 50 add
  50 exch moveto

  dup
  150 mul 150 add
  50 exch lineto

  dup
  150 mul 150 add
  150 exch lineto

  dup
  150 mul 50 add
  150 exch lineto

  dup
  150 mul 50 add
  50 exch lineto

  2 setlinewidth
  0.16 mul 0.2 add
  dup
  1 3 1 roll setrgbcolor
  stroke
} for
